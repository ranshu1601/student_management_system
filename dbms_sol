{ "ok" : 1 }
> db.books.insertMany([{bookno:1,pages:200,name:"idk"},
... {bookno:2,pages:300,name:"idmk"},
... {bookno:3,pages:870,name:"idfk"},
... {bookno:4,pages:560,name:"idc"},
... {bookno:5,pages:87,name:"idfc"},
... {bookno:6,pages:250,name:"iddc"},
... {bookno:7,pages:660,name:"iffdk"},
... {bookno:8,pages:901,name:"ik"},
... {bookno:9,pages:780,name:"ikr"}
... ])


> var map = function() {  
    var category;  
    if ( this.pages >= 250 )   
    category = 'Big Books';  
    else   
    category = "Small Books";  
    emit(category, {name: this.name});  
    };  

> var reduce = function(key, values) {  
    var sum = 0;  
    values.forEach(function(doc) {  
    sum += 1;  
    });  
    return {books: sum};  
    };  

> var count  = db.books.mapReduce(map, reduce, {out: "book_results"});  
    > db[count.result].find()  

